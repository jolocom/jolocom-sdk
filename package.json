{
  "name": "@jolocom/sdk",
  "version": "0.16.1",
  "main": "js/index.js",
  "files": [
    "js/"
  ],
  "scripts": {
    "build": "ttsc -p .",
    "prepare": "yarn build",
    "clean:node": "rm -rf node_modules",
    "format": "eslint --fix --ext .ts --ext .tsx .",
    "repl": "REPL=1 ts-node -P . -r ./bin/repl.ts",
