    "form-data": "^3.0.0",
    "ramda": "^0.26.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "Jolocom",
    "Self-Sovereign Identity",
    "jolocom-sdk",
    "typeorm"
  ]
